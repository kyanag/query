{
    "name": "kyanag/query",
    "description": "一个简单的，laravel风格的查询构造器",
    "version": "1.0.0-dev",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kyanag\\Query\\": "./src"
        },
        "files": [
            "./src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Kyanag\\Query\\Tests\\": "./tests"
        }
    },
    "require": {
        "php": ">=7.4",
        "latitude/latitude": "^4.2",
        "ext-pdo": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "^10.5"
    },
    "scripts": {
        "cs": "phpcs",
        "phpcbf": "phpcbf",
        "phpunit": "phpunit"
    }
}
