<phpunit
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="https://schema.phpunit.de/10.5/phpunit.xsd"
        backupGlobals="true"
        backupStaticAttributes="false"
        cacheTokens="false"
        colors="false"
        processIsolation="false"
        verbose="false">
    <testsuites>
        <testsuite name="QueryBuilders">
            <directory suffix="Test.php">tests/QueryBuilders/</directory>
        </testsuite>
    </testsuites>
</phpunit>